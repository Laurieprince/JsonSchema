{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "schemaLocation",
	"title": "Diagram",
	"description": "A diagram",
	"type": "object",
	"properties": {
		"diagram": {
			"enum": [
				"ClassDiagram",
				"SequenceDiagram",
				"UseCaseDiagram",
				"ObjectDiagram",
				"StateDiagram"
			]
		},
		"nodes": {
			"type": "array",
			"description": "string",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"x": {
						"type": "number"
					},
					"y": {
						"type": "number"
					},
					"id": {
						"type": "number"
					},
					"methods": {
						"type": "string"
					},
					"contents": {
						"type": "string"
					},
					"attributes": {
						"type": "string"
					},
					"children": {
						"type": "array",
						"items": {
							"type": "number"
						}
					},
					"value": {
						"type": "string"
					},
					"openBottom": {
						"type": "boolean"
					},
					"type": {
						"enum": [
							"PointNode",
							"InitialStateNode",
							"FinalStateNode",
							"CallNode",
							"PackageDescriptionNode",
							"PackageNode",
							"ObjectNode",
							"FieldNode",
							"NoteNode",
							"ActorNode",
							"UseCaseNode",
							"StateNode",
							"ImplicitParameterNode",
							"ClassNode",
							"InterfaceNode"
						]
					}
				}
			},
			"anyOf": [
				{
					"properties": {
						"type": {
							"const": [
								"PointNode",
								"InitialStateNode",
								"FinalStateNode"
							]
						}
					},
					"required": [
						""
					]
				},
				{
					"properties": {
						"type": {
							"const": "CallNode"
						}
					},
					"required": [
						"openBottom"
					]
				},
				{
					"properties": {
						"type": {
							"const": "PackageDescriptionNode"
						}
					},
					"required": [
						"contents"
					]
				},
				{
					"properties": {
						"type": {
							"const": "PackageNode"
						}
					},
					"required": [
						""
					]
				},
				{
					"properties": {
						"type": {
							"const": "ObjectNode"
						}
					},
					"required": [
						"children",
						"name"
					]
				},
				{
					"properties": {
						"type": {
							"const": "FieldNode"
						}
					},
					"required": [
						"value",
						"name"
					]
				},
				{
					"properties": {
						"type": {
							"const": "ClassNode"
						}
					},
					"required": [
						"attributes",
						"name",
						"methodds"
					]
				},
				{
					"properties": {
						"type": {
							"const": "InterfaceNode"
						}
					},
					"required": [
						"name",
						"methodds"
					]
				},
				{
					"properties": {
						"type": {
							"const": [
								"NoteNode",
								"ActorNode",
								"UseCaseNode",
								"StateNode",
								"ImplicitParameterNode"
							]
						}
					},
					"required": [
						"name"
					]
				}
			]
		},
		"edges": {
			"type": "array",
			"description": "string",
			"items": {
				"type": "object",
				"properties": {
					"Generalization Type": {
						"enum": [
							"Inheritance",
							"Implementation"
						]
					},
					"directionality": {
						"enum": [
							"Unspecified",
							"Unidirectional",
							"Bidirectional"
						]
					},
					"Aggregation Type": {
						"enum": [
							"Aggregation",
							"Composition"
						]
					},
					"startLabel": {
						"type": "string"
					},
					"middleLabel": {
						"type": "string"
					},
					"endLabel": {
						"type": "string"
					},
					"start": {
						"type": "number"
					},
					"end": {
						"type": "number"
					},
					"type": {
						"enum": [
							"AggregationEdge",
							"AssociationEdge",
							"DependencyEdge",
							"CallEdge",
							"ConstructorEdge",
							"ObjectCollaborationEdge",
							"ObjectReferenceEdge",
							"ReturnEdge",
							"StateTransitionEdge",
							"GeneralizationEdge",
							"NoteEdge",
							"UseCaseAssociationEdge",
							"UseCaseDependencyEdge",
							"UseCaseGeneralizationEdge"
						]
					}
				},
				"required": [
					"start",
					"end"
				]
			},
			"anyOf": [
				{
					"properties": {
						"type": {
							"const": "AggregationEdge"
						}
					},
					"required": [
						"Aggregation Type",
						"startLabel",
						"middleLabel",
						"endLabel"
					]
				},
				{
					"properties": {
						"type": {
							"const": "AssociationEdge"
						}
					},
					"required": [
						"directionality",
						"startLabel",
						"middleLabel",
						"endLabel"
					]
				},
				{
					"properties": {
						"type": {
							"const": "DependencyEdge"
						}
					},
					"required": [
						"directionality",
						"middleLabel"
					]
				},
				{
					"properties": {
						"type": {
							"const": "GeneralizationEdge"
						}
					},
					"required": [
						"Generalization Type"
					]
				},
				{
					"properties": {
						"type": {
							"const": [
								"CallEdge",
								"ConstructorEdge",
								"ObjectCollaborationEdge",
								"ObjectReferenceEdge",
								"ReturnEdge",
								"StateTransitionEdge"
							]
						}
					},
					"required": [
						"middleLabel"
					]
				},
				{
					"properties": {
						"type": {
							"const": [
								"NoteEdge",
								"UseCaseAssociationEdge",
								"UseCaseDependencyEdge",
								"UseCaseGeneralizationEdge"
							]
						}
					},
					"required": [
						"start",
						"end"
					]
				}
			]
		},
		"version": {
			"type": "string"
		}
	}
}